{
  "lerna": "4.0.9",
  "packages": [
    "packages/design-tokens",
    "packages/matchbox-icons",
    "packages/matchbox",
    "packages/matchbox-media",
    "packages/matchbox-css",
    "packages/cra-template-matchbox",
    "packages/next-template-matchbox"
