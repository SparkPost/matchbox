{
  "name": "@sparkpost/matchbox",
  "version": "2.4.4",
  "description": "A React UI component library",
  "main": "matchbox.js",
  "module": "matchbox.es.js",
  "author": "SparkPost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SparkPost/matchbox"
  },
  "homepage": "https://github.com/SparkPost/matchbox/tree/master/packages/matchbox#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "babel-node scripts/build.js --presets=node6",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 6006",
    "deploy-storybook": "storybook-to-ghpages",
    "build-storybook": "build-storybook",
    "lint": "eslint src"
  },
  "sideEffects": false,
  "dependencies": {
    "@sparkpost/matchbox-icons": "^1.0.0",
    "classnames": "^2.2.5",
    "react": "^16.3.2",
    "react-day-picker": "^6.1.0",
    "react-dom": "^16.3.2",
    "react-transition-group": "^2.3.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-backgrounds": "^3.3.10",
    "@storybook/addon-info": "^3.3.9",
    "@storybook/addon-links": "^3.3.10",
    "@storybook/addon-options": "^3.2.6",
    "@storybook/addons": "^3.3.10",
    "@storybook/react": "^3.4.2",
    "@storybook/storybook-deployer": "^2.0.0",
    "autoprefixer": "^7.2.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.24.1",
    "change-case": "^3.0.1",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-config-sparkpost": "^1.3.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.37.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "fs-extra": "^3.0.1",
    "incstr": "^1.2.3",
    "jest": "^20.0.3",
    "lerna": "^2.10.2",
    "node-sass": "^4.5.2",
    "postcss-loader": "^2.0.10",
    "postcss-modules": "^0.7.0",
    "postcss-modules-scope": "^1.0.2",
    "prop-types": "^15.5.10",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^1.5.1",
    "rollup-plugin-uglify": "^3.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.17.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "phantomjs": true
    },
    "rules": {
      "max-lines": [
        "warn",
        {
          "max": 200,
          "skipComments": true
        }
      ],
      "complexity": "off",
      "one-var": [
        "error",
        "never"
      ],
      "object-curly-spacing": [
        "error",
        "always",
        {
          "arraysInObjects": false,
          "objectsInObjects": false
        }
      ],
      "block-spacing": [
        "error",
        "always"
      ],
      "operator-linebreak": [
        "error",
        "after",
        {
          "overrides": {
            "?": "before",
            ":": "before"
          }
        }
      ]
    },
    "extends": [
      "react-app",
      "sparkpost"
    ]
  }
}
